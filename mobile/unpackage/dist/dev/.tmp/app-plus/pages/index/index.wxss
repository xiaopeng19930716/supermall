
.cycle {
	width: 100rpx;
	height: 100rpx;
	border-radius: 50rpx;
	text-align: center;
	line-height: 100rpx;
	background-color: #aa55ff;
}
.active {
	background: #55aaff;
}
.sign-title {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 30rpx 24rpx;
	border-bottom: 1rpx solid #333;
}
.sign-title .portrait {
	width: 100rpx;
	height: 100rpx;
	line-height: 100rpx;
	border-radius: 50%;
	background-color: #007aff;
	color: #fff;
	font-size: 28rpx;
	text-align: center;
}
.sign-title .sign-title-l {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.sign-title .sign-title-l .text {
	margin-left: 20rpx;
}
.sign-title .sign-title-l .text .name {
	font-size: 32rpx;
}
.sign-title .sign-title-l .text .gz {
	color: darkblue;
	display: -webkit-inline-box;
	display: -webkit-inline-flex;
	display: inline-flex;
}
.sign-title .sign-title-l .text .gz text {
	display: inline-block;
}
.sign-title .sign-title-l .text .gz .t1 {
	overflow: hidden; /*超出部分隐藏*/
	text-overflow: ellipsis; /* 超出部分显示省略号 */
	white-space: nowrap; /*规定段落中的文本不进行换行 */
	width: 166rpx; /*需要配合宽度来使用*/
}


.uni-steps {
